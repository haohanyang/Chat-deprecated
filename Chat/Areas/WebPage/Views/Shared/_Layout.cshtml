<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Boostrap Icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/haohanyang/AdminLTE@master/dist/css/adminlte.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

</head>

<body class="hold-transition layout-top-nav">
    <div class="wrapper">

        @RenderSection("Header", required: false)
        @RenderBody()

        <aside class="control-sidebar control-sidebar-dark">
  
        </aside>

        <footer class="main-footer">
            <div class="float-right d-none d-sm-inline">
                Anything you want
            </div>
            <strong>Copyright &copy; 2014-2021 <a href="https://adminlte.io">AdminLTE.io</a>.</strong> All rights
            reserved.
        </footer>
    </div>

    <script src="https://cdn.jsdelivr.net/gh/haohanyang/AdminLTE@master/plugins/jquery/jquery.min.js"></script>
    <script
        src="https://cdn.jsdelivr.net/gh/haohanyang/AdminLTE@master/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/haohanyang/AdminLTE@master/dist/js/adminlte.min.js"></script>
    @await RenderSectionAsync("Scripts", false)
</body>

</html>